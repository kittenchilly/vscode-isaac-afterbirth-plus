{
	"name": "vscode-isaac-repentance",
	"displayName": "vscode-isaac-repentance",
	"description": "Provides auto-completion names for programming The Binding of Isaac: Repentance mods.",
	"version": "0.0.1",
	"publisher": "kittenchilly, Zamiel",
	"repository": {
		"type": "git",
		"url": "https://github.com/kittenchilly/vscode-isaac-repentance"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
